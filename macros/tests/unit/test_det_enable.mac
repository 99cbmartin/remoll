# This must be called before initialize
/remoll/setgeofile geometry_sculpt/mollerMother.gdml

# This must be explicitly called
/run/initialize

# Field maps
/remoll/addfield map_directory/blockyHybrid_rm_3.0.txt
/remoll/addfield map_directory/blockyUpstream_rm_1.1.txt

# Raster and initial angle stuff
/remoll/oldras true
/remoll/rasx 5 mm
/remoll/rasy 5 mm

# Beam energy
/remoll/beamene 11 GeV


#/remoll/gen beam
/remoll/gen moller
#/remoll/thcommin 30.0 deg
#/remoll/thcommax 150.0 deg
#/remoll/gen elastic 
#/remoll/emin 80.0 MeV
#/remoll/gen inelastic 
#/remoll/gen pion
#/remoll/piontype pi-
#/remoll/gen pion_LUND
#/remoll/gen inelasticAl
#/remoll/gen quasielasticAl
#/remoll/gen elasticAl

# Generation limits
# theta
/remoll/thmin 0.5 deg
/remoll/thmax 3.0 deg

# Beam current for rate
/remoll/beamcurr 85 microampere

/remoll/seed 123456

# Output
/remoll/filename test_det_enable_1.root

# Run
/run/beamOn 1000

# Disable pion detectors
/remoll/SD/det2001/enable false
/remoll/SD/det2002/enable false
/remoll/SD/det2003/enable false
/remoll/SD/det2004/enable false

# Output
/remoll/filename test_det_enable_2.root

# Run
/run/beamOn 1000
