  </volume>

  <volume name="col2USVirtualDet_log">
    <materialref ref="G4_Galactic"/>
    <solidref ref="EnvelopeDet_solid"/>
    <auxiliary auxtype="SensDet" auxvalue="planeDet"/>
    <auxiliary auxtype="DetNo" auxvalue="47"/>
  </volume>
  
  <volume name="col2DSVirtualDet_log">
    <materialref ref="G4_Galactic"/>
    <solidref ref="EnvelopeDet_solid"/>
    <auxiliary auxtype="SensDet" auxvalue="planeDet"/>
