      <positionref ref="n2atmoVirtualPlane_pos"/>
    </physvol>

    <physvol name="col2USVirtualDet_phys">
      <volumeref ref="col2USVirtualDet_log"/>
      <position name="col2USVirtualDet_pos" z="750-0.6" unit="mm"/>
    </physvol>

    <physvol name="col2DSVirtualDet_phys">
      <volumeref ref="col2DSVirtualDet_log"/>
      <position name="col2DSVirtualDet_pos" z="900+0.6" unit="mm"/>
    </physvol>

    <physvol name="col4USVirtualDet_phys">
      <volumeref ref="col4USVirtualDet_log"/>
      <position name="col4USVirtualDet_pos" z="3225-0.6" unit="mm"/>
    </physvol>

    <physvol name="col4DSVirtualDet_phys">
      <volumeref ref="col4DSVirtualDet_log"/>
      <position name="col4DSVirtualDet_pos" z="3375+0.6" unit="mm"/>
