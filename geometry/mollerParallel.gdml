    </physvol>

    <physvol name="col5USVirtualDet_phys">
      <volumeref ref="col5USVirtualDet_log"/>
      <position name="col5USVirtualDet_pos" z="7785-0.6" unit="mm"/>
    </physvol>

    <physvol name="col5DSVirtualDet_phys">
      <volumeref ref="col5DSVirtualDet_log"/>
      <position name="col5DSVirtualDet_pos" z="7885+0.6" unit="mm"/>
    </physvol>

    <physvol name="coll1USVirtualDet_phys">
      <volumeref ref="coll1USVirtualDet_log"/>
      <position name="coll1USVirtualDet_pos" z="12250-0.6" unit="mm"/>
    </physvol>

    <physvol name="coll1DSVirtualDet_phys">
      <volumeref ref="coll1DSVirtualDet_log"/>
      <position name="coll1DSVirtualDet_pos" z="12400+0.6" unit="mm"/>
    </physvol>

    <physvol name="coll2USVirtualDet_phys">
      <volumeref ref="coll2USVirtualDet_log"/>
      <position name="coll2USVirtualDet_pos" z="18850-0.6" unit="mm"/>
