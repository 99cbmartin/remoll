  <volume name="HybridPbUSVirtualDet_log">
    <materialref ref="G4_Galactic"/>
    <solidref ref="EnvelopeDet_solid"/>
    <auxiliary auxtype="SensDet" auxvalue="planeDet"/>
    <auxiliary auxtype="DetNo" auxvalue="51"/>
  </volume>
  
  <volume name="HybridPbDSVirtualDet_log">
    <materialref ref="G4_Galactic"/>
    <solidref ref="EnvelopeDet_solid"/>
    <auxiliary auxtype="SensDet" auxvalue="planeDet"/>
    <auxiliary auxtype="DetNo" auxvalue="52"/>
  </volume>
  
  <volume name="col5USVirtualDet_log">
    <materialref ref="G4_Galactic"/>
    <solidref ref="EnvelopeDet_solid"/>
